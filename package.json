{
  "name": "discordjs-testing",
  "version": "0.0.0",
  "description": "Helper package for testing DiscordJS bots with Mocha/Chai",
  "main": "prod/index.ts",
  "scripts": {
    "prebuild": "rm -rf prod",
    "build": "tsc",
    "lint": "eslint src test --max-warnings 0",
    "start": "node -r dotenv/config prod/index.js",
    "test": "ts-mocha -u tdd  test/**/*.spec.ts --recursive --exit --timeout 10000"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/naomi-lgbt/discord-testing.git"
  },
  "keywords": [
    "typescript",
    "discord.js",
    "discord",
    "testing",
    "mocks"
  ],
  "author": "Naomi Carrigan",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/naomi-lgbt/discord-testing/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/naomi-lgbt"
  },
  "homepage": "https://github.com/naomi-lgbt/discord-testing#readme",
  "dependencies": {
    "dotenv": "16.0.3",
    "winston": "3.8.2"
  },
  "devDependencies": {
    "@nhcarrigan/eslint-config": "1.1.2",
    "@nhcarrigan/prettier-config": "1.0.1",
    "@nhcarrigan/typescript-config": "1.0.1",
    "@types/chai": "4.3.5",
    "@types/mocha": "10.0.1",
    "@types/node": "18.16.3",
    "chai": "4.3.7",
    "eslint": "8.39.0",
    "mocha": "10.2.0",
    "prettier": "2.8.8",
    "ts-mocha": "10.0.0",
    "typescript": "5.0.4"
  },
  "peerDependencies": {
    "discord.js": "14.10.2"
  }
}